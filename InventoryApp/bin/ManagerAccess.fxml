<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="266.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ManagerAccessController">
   <children>
      <TextField fx:id="txtUserID" layoutX="14.0" layoutY="66.0" />
      <TextField fx:id="txtPassword" layoutX="14.0" layoutY="125.0" />
      <TextField fx:id="txtPassword2" layoutX="14.0" layoutY="175.0" />
      <Button fx:id="btnAdd" layoutX="14.0" layoutY="227.0" mnemonicParsing="false" onAction="#btnAdd" text="Add" />
      <Button fx:id="btnClose" layoutX="302.0" layoutY="227.0" mnemonicParsing="false" onAction="#closeAction" text="Finish" />
      <Label layoutX="14.0" layoutY="49.0" text="UserID" />
      <Label layoutX="18.0" layoutY="104.0" text="Password (if changing or new user)" />
      <Label layoutX="14.0" layoutY="157.0" text="Re-Enter Password" />
      <Label layoutX="219.0" layoutY="159.0" text="User Access Level" />
      <ComboBox fx:id="userLevelDrop" layoutX="219.0" layoutY="175.0" prefWidth="150.0" />
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="381.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="menuSave" mnemonicParsing="false" onAction="#saveAction" text="Save" />
                  <MenuItem fx:id="menuClose" mnemonicParsing="false" onAction="#closeAction" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="btnUpdate" layoutX="150.0" layoutY="227.0" mnemonicParsing="false" onAction="#btnUpdate" text="Update" />
   </children>
</AnchorPane>
